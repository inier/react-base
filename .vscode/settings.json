{
    "path-autocomplete.excludedItems": {
        "**/*.js": {
            "when": "**/*.ts"
        },
        "**/*.map": {
            "when": "**"
        },
        "**/{.git,node_modules}": {
            "when": "**"
        }
    },
    "path-autocomplete.pathMappings": {
        "@": "${workspaceRoot}/src"
    },
    "path-autocomplete.extensionOnImport": true,
    "cSpell.words": [
        "alifd",
        "alifd theme chm",
        "bizcharts",
        "browserslist",
        "chm",
        "corejs",
        "css",
        "css mqpacker",
        "eslint",
        "eslint plugin flowtype",
        "flowtype",
        "mixins",
        "mqpacker",
        "ozo",
        "ozo mixins sass",
        "plugin",
        "predeploy",
        "sass",
        "theme"
    ]
}